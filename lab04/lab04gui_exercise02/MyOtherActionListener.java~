import java.awt.*;
import java.awt.event.*;

public class MyOtherActionListener extends ActionListener
{
   
   //constants
   //variables 
   
   //methods 
   public void actionPerformed( ActionEvent e)
   {
      System.out.println( "Button Pressed");
      if ( e.getOperation().equals( "Maybe"))
      {
         ( e.getSource()).setText( "OK");
      }
      else
      {
         ( e.getSource()).setText( "Maybe");
      }
   }
}
   